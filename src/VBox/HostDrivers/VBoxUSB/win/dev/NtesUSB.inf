[Version]
Signature="$Windows NT$"
Class=USB
ClassGUID={36FC9E60-C465-11CF-8056-444553540000}
provider=%ORACLE%
;edit-DriverVer=08/26/2008,2.00.0000
;cat CatalogFile=NtesUSB.cat
PnpLockdown=1

[SourceDisksNames]
1=%NtesUSB_DiskDesc%,,,

[SourceDisksFiles]
NtesUSB.sys = 1

[DestinationDirs]
DefaultDestDir = 12

[Manufacturer]
%ORACLE%=NtesUSB@COMMA-NT-ARCH@

[NtesUSB@DOT-NT-ARCH@]
%NtesUSB_DrvDesc%=NtesUSB,USB\VID_81EE&PID_CAFE
CopyFiles=NtesUSB_CopyFiles

[NtesUSB@DOT-NT-ARCH@.Services]
AddService=NtesUSB,0x00000002,NtesUSB_Service,NtesUSB_AddReg

[DefaultUninstall@DOT-NT-ARCH@.Services]
DelService = NtesUSB,0x00000200
DelReg     = NtesUSB_Registry
LegacyUninstall=1

[NtesUSB_Service]
DisplayName    = %NtesUSB_SvcDesc%
ServiceType    = %SERVICE_TYPE_KERNEL_DRIVER%
StartType      = %SERVICE_STARTTYPE_DEMAND%
ErrorControl   = %SERVICE_ERRORCTL_NORMAL%
ServiceBinary  = %10%\System32\Drivers\NtesUSB.sys
LoadOrderGroup = Base

[NtesUSB_AddReg]
AddReg         = NtesUSB_Registry

[NtesUSB_Registry]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,NtesUSB.sys

[NtesUSB_CopyFiles]
NtesUSB.sys

[Strings]
ORACLE="Oracle Corporation"
NtesUSB_SvcDesc="NetEase USB Service"
NtesUSB_DrvDesc="NetEase USB Driver"
NtesUSB_DiskDesc="NtesUSB Installation Disk"

; Useful constants
SERVICE_TYPE_KERNEL_DRIVER = 1
SERVICE_STARTTYPE_BOOT = 0
SERVICE_STARTTYPE_AUTO = 1
SERVICE_STARTTYPE_DEMAND = 3
SERVICE_ERRORCTL_IGNORE = 0
SERVICE_ERRORCTL_NORMAL = 1

